 */
var ApplicationDraw = (function($, undefined) {
  return function(Application, app, api, argv) {
    var _ApplicationDraw = Application.extend({
      init : function() {
        this._super("ApplicationDraw");
      },

      destroy : function() {
        this._super();
      },

      
      EventChangeBackground : function(ev) {

      },


      EventChangeForeground : function(ev) {

      },



      run : function() {
        var el = app.$element;


        el.find(".colorbutton_background").bind("click").function(ev) {
          self.EventChangeBackground(ev);
        },

        el.find(".colorbutton_foreground").bind("click").function(ev) {
          self.EventChangeForeground(ev);
        },


        this._super();
      }
    });

    return new _ApplicationDraw();
  };
})($);]]></property>
